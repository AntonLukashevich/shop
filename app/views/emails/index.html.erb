<h1>Listing emails</h1>


<%= link_to 'New email', new_email_path %>
<table>
  <tr>
    <th>Title</th>
    <th></th>
  </tr>

  <% @emails.each do |email| %>
    <tr>
      <td><%= email.email %></td>
      <td><%= link_to 'Show', email_path(email) %></td>
      <td><%= link_to 'Edit', edit_email_path(email) %></td>
      <td><%= link_to 'Destroy', email_path(email),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>

</table>
<%= link_to 'Home', controller: 'welcome' %>
