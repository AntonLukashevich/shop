<h1>Edit Email</h1>

<%= form_with model: @email,local: true do |form| %>
  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
<br>
    <%= form.label :user_id %><br>
    <%= form.text_field :user_id %>
  </p>

  <p>
    <%= form.submit %>
  </p>

<% end %>

<%= link_to 'Back',emails_path %>